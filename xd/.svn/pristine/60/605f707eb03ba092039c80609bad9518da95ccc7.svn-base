        cultureModuleList: [],
        healthModuleList: [],
        isOfflineActivities: "",
        solveTheProblem: "",
        other: ""
      }
    };
  },
  methods: {
    add() {
      if (this.datobj.age != "") {
        addlist(this.datobj)
          .then(res => {
            if (res && res.errorCode == 0) {
              this.$message({
                showClose: true,
                message: "添加成功",
                type: "success"
              });
            }
            this.$refs.ruleForm.resetFields();
            this.datobj.other="";
          })
          .catch(() => {});
      } else {
        this.$message({
          showClose: true,
          message: "请填写信息",
          type: "success"
        });
      }
    },
  }
};
